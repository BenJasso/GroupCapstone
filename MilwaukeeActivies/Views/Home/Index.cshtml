@model ActivitiesAPI.

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome to Milwaukee Activities!</h1>
    <br />
    <p>Here's some things that are going on around you!</p>
    <br />
    <p>If you are a returning user, login to enhance your experience!</p>
    <br />
</div>

<head>
    <style>
        #map {
            height: 500px;
            width: 100%;
        }
    </style>
</head>
<body>
    <div id="map"></div>
    <script>
        function initMap() {
            var milwaukee = { lat: 43.0389, lng: -87.9065 };
            var map = new google.maps.Map(document.getElementById('map'), { zoom: 10, center: milwaukee });
            var marker = new google.maps.Marker({ position: milwaukee, map: map });
        }
    </script>
    <script async defer
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAGo1Cx5-3NVQ1v599tNS2j3BVgXZtSNa0&callback=initMap">
    </script>
</body>
